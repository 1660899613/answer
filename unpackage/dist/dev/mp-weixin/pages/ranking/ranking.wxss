.content {
  display: flex;
  flex-direction: column;
}
.answer {
  margin: 0 auto;
  background: rgba(0, 0, 0, 0.2);
  display: flex;
  justify-content: center;
  padding: 10rpx 0;
}
.answer .answer_content {
  width: 653rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: cover;
  padding-bottom: 100rpx;
}
.answer .answer_content .subject {
  width: 615rpx;
  background: #FFFFFF;
  margin: 80rpx auto;
  padding: 30rpx 0;
  position: relative;
  font-size: 30rpx;
  font-weight: 400;
  color: #232323;
  display: flex;
  justify-content: space-between;
}
.answer .answer_content .subject .sub_left {
  width: 218rpx;
  height: 186rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  border-right: 1px dotted #dedede;
}
.answer .answer_content .subject .sub_left .sub_name {
  font-size: 40rpx;
  font-weight: 600;
  color: #D81D1D;
}
.answer .answer_content .subject .sub_left .sub_rank {
  font-size: 20rpx;
  font-weight: 400;
  color: #474747;
}
.answer .answer_content .subject .sub_left .sub_rank .bai_sub {
  color: #E32626;
}
.answer .answer_content .subject .sub_left .TOPnum {
  padding: 0 15rpx;
  background: #E32626;
  border-radius: 10rpx;
  color: #FFFFFF;
  margin-top: 15rpx;
}
.answer .answer_content .subject .sub_right {
  width: 395rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.answer .answer_content .subject .sub_right .sub_name_title {
  font-size: 30rpx;
  font-weight: 400;
  color: #474747;
}
.answer .answer_content .subject .sub_right .sub_integral {
  font-size: 26rpx;
  font-family: Microsoft YaHei;
  font-weight: 400;
  color: #575755;
}
.answer .answer_content .subject .sub_right .sub_integral .integral_num {
  color: #D81D1D;
}
.answer .answer_content .subject .sub_right .rankeines {
  font-size: 28rpx;
  font-weight: 400;
  color: #474747;
}
.answer .answer_content .subject .sub_right .rankeines .Rownumber {
  color: #E32626;
  font-weight: bold;
  margin-left: 10rpx;
}
.answer .answer_content .subject::after {
  content: "";
  position: absolute;
  bottom: -40rpx;
  left: 0;
  width: 100%;
  height: 30px;
  display: block;
  background: linear-gradient(-45deg, transparent 33.33%, #fff 33.33%, #fff 66.666%, transparent 66.666%), linear-gradient(45deg, transparent 33.33%, #fff 33.33%, #fff 66.666%, transparent 66.666%);
  background-size: 20rpx 60rpx;
  -webkit-transform: rotate(180deg);
          transform: rotate(180deg);
}
.answer .answer_content .subject::before {
  content: "";
  position: absolute;
  top: -40rpx;
  left: 0;
  width: 100%;
  height: 30px;
  display: block;
  background: linear-gradient(-45deg, transparent 33.33%, #fff 33.33%, #fff 66.666%, transparent 66.666%), linear-gradient(45deg, transparent 33.33%, #fff 33.33%, #fff 66.666%, transparent 66.666%);
  background-size: 20rpx 60rpx;
}
.answer .answer_content .num_now {
  width: 595rpx;
  margin: 0 auto;
}
.answer .answer_content .num_now .num_list {
  width: 100%;
  height: 78rpx;
  background: #FFFFFF;
  margin-bottom: 38rpx;
  display: flex;
  overflow: hidden;
}
.answer .answer_content .num_now .num_list .num_inet {
  width: 86rpx;
  height: 100%;
  background: #E32626;
  color: #FFFFFF;
  font-size: 36rpx;
  font-weight: 400;
  display: flex;
  align-items: center;
  justify-content: center;
}
.answer .answer_content .num_now .num_list .class_inet_name {
  width: 345rpx;
  padding: 0 10rpx;
  line-height: 78rpx;
  color: #454545;
  font-size: 30rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.answer .answer_content .num_now .num_list .frac_num {
  display: flex;
  align-items: center;
  justify-content: center;
}
.answer .answer_content .num_now .num_list .frac_num .frac_img {
  width: 39rpx;
  height: 39rpx;
}
.answer .answer_content .num_now .num_list .frac_num .farc_intent {
  font-size: 26rpx;
  font-weight: 400;
  color: #454545;
  margin-left: 15rpx;
}
.answer .answer_content .num_now .active {
  background: #E32626;
  color: #FFFFFF;
}
.answer .answer_content .num_now .active .num_inet {
  background: #FFFFFF;
  color: #E32626;
}
.answer .answer_content .num_now .active .class_inet_name {
  color: #FFFFFF;
}
.answer .answer_content .num_now .active .frac_num .farc_intent {
  color: #FFFFFF;
}

